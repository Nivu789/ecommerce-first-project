                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="row gx-3 row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-xl-4 row-cols-xxl-5">
                        <%for(i=0;i<productData.length;i++){%>
                            <div class="col">
                                <div class="card card-product-grid">
                                    <a href="#" class="img-wrap"> <img src="/<%=productData[i].image[0]%>" alt="Product"> </a>
                                    <div class="info-wrap">
                                        <a href="#" class="title text-truncate"><%=productData[i].productName%></a>
                                        <div class="price mb-2"><%=productData[i].regularPrice%></div>
                                        <a href="/admin/editproduct?id=<%=productData[i]._id%>" class="btn btn-sm font-sm rounded btn-brand">
                                            <i class="material-icons md-edit"></i> Edit
                                        </a>
                                        
                                        <%if(productData[i].is_active==true){%>
                                        <a href="/admin/blockproduct?id=<%=productData[i]._id%>" class="btn btn-sm font-sm btn-danger rounded">
                                            <i class="material-icons md-delete_forever"></i> Block
                                        </a>
                                        <%}else{%>
                                            <a href="/admin/unblockproduct?id=<%=productData[i]._id%>" class="btn btn-sm font-sm btn-success rounded">
                                                <i class="material-icons md-delete_forever"></i> Unblock
                                            </a>
                                            <%}%>
                                    </div>
                                </div> 
                            </div> 
                            <%}%>
                            
                    </div> <!-- row.// -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
