<body>
    <div class="screen-overlay"></div>
    <%-include('aside.ejs')-%>
    <main class="main-wrap">
        <%-include('navbar.ejs')%>
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Transactions </h2>
                <button class="btn btn-primary" onclick="printReport()">Print Report</button>
            </div>
            
            
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-9">
                            <header class="border-bottom mb-4 pb-4">
                                <form method="post" action="/admin/filter-report">
                                <div class="row">
                                    <div class="col-lg-3 col-6 me-auto">
                                        <input type="text" name="search" placeholder="Search..." class="form-control  hidden-on-print">
                                    </div>
                                    <div class="col-lg-3 col-6 me-auto">
                                        <label for="">From</label>
                                        <input type="date" name="from_date" class="form-control" max="<%= (new Date()).toISOString().split('T')[0] %>">
                                        
                                    </div>
                                    <div class="col-lg-3 col-6 me-auto">
                                        <label for="">To</label>
                                        <input type="date" name="to_date" class="form-control" max="<%= (new Date()).toISOString().split('T')[0] %>">
                                        
                                    </div>
                                    <div class="col-lg-2 col-6">
                                        <button class="btn btn-primary" type="submit">Filter</button>
                                    </div>
                                </div>
                            </form>
                            </header> <!-- card-header end// -->

                            <div class="table-responsive">
                                <table class="table table-hover table-primary" style="border: 2px solid black;">
                                    <thead>
                                        <tr>
                                            <th>Total No.of orders</th>
                                            <th>Total No. of customers</th>
                                            <th>Total No.of online payments </th>
                                            <th>total No. of cash on delivery</th>
                                            <th> Total No. of orders cancelled </th>
                                            <th> Total Transaction </th>
                                        </tr>
                                    </thead>

                                    <tbody style="border: 1px solid black;">
                                        <tr>
                                                <%if(locals.orderData){%>
                                                    <td class="text-center"><%=orderData.length%></td>
                                                    <%}else{%>
                                                        <td class="text-center"><%=filteredData.length%></td>
                                                    <%}%>
                                                    <td class="text-center"><%=distinctUserLength%></td>
                                                    <td class="text-center"><%=onlinePayments%></td>
                                                    <td class="text-center"><%=cashPayments%></td>
                                                    <td class="text-center"><%=cancelledOrders%></td>
                                                    <td class="text-center">₹<%=totalAmount%></td>
                                        </tr>
                                    </tbody>

                                </table>

                            </div>

                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>Order ID</th>
                                            <th>Total Amount</th>
                                            <th>Payment Method</th>
                                            <th> Order Date</th>
                                            <th class="text-end"> Action </th>
                                        </tr>
                                    </thead>
                                    
                                    
                                        <tbody>
                                        
                                            <%for(i=0;i<orderData.length;i++){%>
                                                <tr>
                                                    <td><b><%=orderData[i].orderId%></b></td>
                                                    <td><%=orderData[i].totalAmount%></td>
                                                    <td>
                                                        <div class="icontext">
                                                            <img class="icon border" src="/admin-assets/imgs/card-brands/1.png" alt="Payment">
                                                            <span class="text text-muted"><%=orderData[i].paymentMethod%></span>
                                                        </div>
                                                    </td>
                                                    <td><%=orderData[i].orderDate.toLocaleString()%></td>
                                                    <td class="text-end">
                                                        <a href="/admin/orderdetails?id=<%=orderData[i]._id%>" class="btn btn-sm btn-light font-sm rounded">Details</a>
                                                    </td>
                                                </tr>
                                                
                                                <%}%>
                                            
                                       
                                    </tbody>
                                        
                                           
                                    
                                </table>
                            </div> <!-- table-responsive.// -->
                        </div> <!-- col end// -->
