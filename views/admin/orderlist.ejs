                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="orderTable">
                            <thead>
                                <tr>
                                    <th>OrderId</th>
                                    <th scope="col">Name</th>
                                    <th scope="col" style="text-align: center;">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(i=0;i<orderData.length;i++){%>
                                    <tr>
                                        <td><%=orderData[i].orderId%></td>
                                        <td><b><%=orderData[i].userId.name%></b></td>
                                        <td style="text-align: center;"><%=orderData[i].userId.email%></td>
                                        <td><%=orderData[i].totalAmount%></td>
                                        <%if(orderData[i].orderStatus==="Shipped"){%>
                                            <td><span class="badge rounded-pill alert-success"><%=orderData[i].orderStatus%></span></td>
                                            <%}else if(orderData[i].orderStatus==="Cancelled"){%>
                                            <td><span class="badge rounded-pill alert-danger"><%=orderData[i].orderStatus%></span></td>
                                            <%}else{%>
                                                <td><span class="badge rounded-pill alert-warning"><%=orderData[i].orderStatus%></span></td>
                                                <%}%>
                                        <td><%=orderData[i].orderDate.toLocaleDateString()%></td>
                                        <td class="text-end">
                                            <a href="/admin/orderdetails?id=<%=orderData[i]._id%>" class="btn btn-md rounded font-sm">Detail</a>
                                            
                                        </td>
                                    </tr>
                                    <%}%>
                                
                                </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
