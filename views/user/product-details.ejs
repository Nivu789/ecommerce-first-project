                                                            function submitReply(productId) {
                                                                console.log(productId)
                                                                const reviewReply = document.getElementById('review_reply').value

                                                                fetch('/reply-to-review', {
                                                                    method: "POST",
                                                                    headers: {
                                                                        'Content-Type': 'application/json'
                                                                    },
                                                                    body: JSON.stringify({
                                                                        reviewId: reviewid,
                                                                        reviewReply: reviewReply,
                                                                        productId: productId
                                                                    })
                                                                }).then(res => {
                                                                    if (res.ok) {
                                                                        console.log("Success")
                                                                        return res.json()
                                                                    } else {
                                                                        console.log("Error")
                                                                    }
                                                                }).then(data => {
                                                                    if (data.message == "Success") {
                                                                        Swal.fire({
                                                                            icon: "success",
                                                                            title: "Reply posted",
                                                                            showConfirmButton: false,
                                                                            timer: 1500
                                                                        });
                                                                        setTimeout(() => {
                                                                            window.location.reload();
                                                                        }, 1500);
                                                                        
                                                                    }
                                                                })
                                                                    .catch(err => console.log(err))
                                                            }
                                                        </script>

